rp-manifest: 1

id: @project_name@
version: @PROJECT_VERSION@
name: @PROJECT_NAME@
description: @PROJECT_DESCRIPTION@
author: @PROJECT_AUTHOR@ <@PROJECT_AUTHOR_MAIL@>
license: @PROJECT_LICENSE@

targets:
  - target: main
    content:
      src: @WIDGET_ENTRY_POINT@
      type: @WIDGET_TYPE@
    icon:
      - src: @PROJECT_ICON@
